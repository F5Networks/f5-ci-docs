{
  "virtualServer": {
    "backend": {
      "servicePort": 3000,
      "serviceName": "frontend",
      "healthMonitors": [{
        "interval": 30,
        "protocol": "http",
        "send": "GET",
        "timeout": 86400
      }]
    },
    "frontend": {
      "virtualAddress": {
        "port": 80,
        "bindAddr": "173.16.2.2"
      },
      "partition": "kubernetes",
      "balance": "round-robin",
      "mode": "http"
    }
  }
}
