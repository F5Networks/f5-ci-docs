{
  "virtualServer": {
    "frontend": {
      "balance": "round-robin",
      "mode": "http",
      "partition": "my-bigip-partition",
      "virtualAddress": {
        "bindAddr": "1.2.3.4",
        "port": 443
      },
      "sslProfile": {
        "f5ProfileName": "Common/my-ssl-cert"
      }
    },
    "backend": {
      "serviceName": "my-kubernetes-service",
      "servicePort": 443
    }
  }
}
