{
  "container": {
    "docker": {
      "portMappings": [
        {
          "servicePort": 11099,
          "protocol": "tcp",
          "containerPort": 80,
          "hostPort": 0
        }
      ],
      "privileged": false,
      "image": "<my-app-image:version>",
      "network": "BRIDGE",
      "forcePullImage": true
    },
    "type": "DOCKER",
    "volumes": []
  },
  "mem": 128,
  "labels": {
    "asp": "enable"
  },
  "cpus": 0.25,
  "instances": 1,
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  },
  // use if pulling from a private docker registry
  "uris": [
    "file:///etc/dockercfg.tgz"
  ],
  "id": "demo_app"
}